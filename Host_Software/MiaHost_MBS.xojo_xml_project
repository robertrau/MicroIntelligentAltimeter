 <ControlBehavior>
  <Superclass>Timer</Superclass>
  <HookInstance>
   <ItemName>Action</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>329945087</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub Action()</SourceLine>
    <SourceLine>'This timer keeps the serial port popup list up to date</SourceLine>
    <SourceLine>'</SourceLine>
    <SourceLine>Var count As Integer = SerialPopup.RowCount</SourceLine>
    <SourceLine>Var i As Integer</SourceLine>
    <SourceLine>Var CurrentSelectedSerialPortName as String</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>If SerialDevice.Count &lt;&gt; count Then</SourceLine>
    <SourceLine>' The number of serial devices has changed so update the menu</SourceLine>
    <SourceLine>SerialPopup.RemoveAllRows</SourceLine>
    <SourceLine>For i = 0 To SerialDevice.LastIndex</SourceLine>
    <SourceLine>SerialPopup.AddRow(SerialDevice.At(i).Name)</SourceLine>
    <SourceLine>ConnectToMiaButton.Enabled = true</SourceLine>
    <SourceLine>Next</SourceLine>
    <SourceLine>If SerialDevice.Count &lt; count Then ' a device has been removed</SourceLine>
    <SourceLine>SerialPopup.SelectedRowIndex = 0</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>CurrentSelectedSerialPortName = SerialPopup.SelectedRowText</SourceLine>
    <SourceLine>if LastSelectedSerialPortName &lt;&gt; CurrentSelectedSerialPortName then</SourceLine>
    <SourceLine>ConnectToMiaButton.Enabled = False</SourceLine>
    <SourceLine>SerialPortNameLabel.Text = "No Connection to Flight Computer"</SourceLine>
    <SourceLine>DisconnectStatusLabel.Text = ""</SourceLine>
    <SourceLine>DisconnectFromMiaButton.Enabled = False</SourceLine>
    <SourceLine>ConnectToMiaButton.Enabled = True</SourceLine>
    <SourceLine>ConnectToMiaButton.Caption = "Connect To Mia"</SourceLine>
    <SourceLine>Serial1.Close</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine>LastSelectedSerialPortName = CurrentSelectedSerialPortName</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Else ' one has been added so select the new device</SourceLine>
    <SourceLine>SerialPopup.SelectedRowIndex = SerialPopup.LastRowIndex</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine>InitialSerialPopupPopulationDone = True</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
 </ControlBehavior>
