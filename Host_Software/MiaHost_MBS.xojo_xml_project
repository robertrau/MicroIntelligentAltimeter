   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function ShowStatus(status as integer) As string</SourceLine>
   <SourceLine>#if TargetWindows Then</SourceLine>
   <SourceLine>dim t as string</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Select case status</SourceLine>
   <SourceLine>case lm.kLocationReportStatusNotSupported</SourceLine>
   <SourceLine>t = "Not supported"</SourceLine>
   <SourceLine>EnableManualLocationEntry</SourceLine>
   <SourceLine>case lm.kLocationReportStatusError</SourceLine>
   <SourceLine>t = "Error"</SourceLine>
   <SourceLine>EnableManualLocationEntry</SourceLine>
   <SourceLine>case lm.kLocationReportStatusAccessDenied</SourceLine>
   <SourceLine>t = "Access Denied"</SourceLine>
   <SourceLine>EnableManualLocationEntry</SourceLine>
   <SourceLine>case lm.kLocationReportStatusInitializing</SourceLine>
   <SourceLine>t = "Initializing"</SourceLine>
   <SourceLine>case lm.kLocationReportStatusRunning</SourceLine>
   <SourceLine>t = "Acquiring latitude and longitude"</SourceLine>
   <SourceLine>MiaHost.AutomaticLocationAvailable = True</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>t = "Unknown status "+str(status)</SourceLine>
   <SourceLine>EnableManualLocationEntry</SourceLine>
   <SourceLine>end Select</SourceLine>
   <SourceLine>LocationInfo.Text = "Location status: " + t</SourceLine>
   <SourceLine>return t</SourceLine>
   <SourceLine>#EndIf</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>status as integer</ItemParams>
  <ItemResult>string</ItemResult>
 </Method>
