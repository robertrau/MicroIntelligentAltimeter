    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Var NumberOfFlightsFound as uint16</SourceLine>
    <SourceLine>Var FlightNumber as uint16</SourceLine>
    <SourceLine>Var ReceivedListOfFlights as string</SourceLine>
    <SourceLine>Var ReceivedFlightLog as string</SourceLine>
    <SourceLine>Var ReceivedCharacters as string</SourceLine>
    <SourceLine>Var StartTimeTicks as Integer</SourceLine>
    <SourceLine>Var EndTimeTicks as Integer</SourceLine>
    <SourceLine>Var LogFields() as string</SourceLine>
    <SourceLine>Var UNIXFlightTime as int64</SourceLine>
    <SourceLine>Var UNIXMiaTime as int64</SourceLine>
    <SourceLine>Var FlightDateTime as DateTime</SourceLine>
    <SourceLine>Var MiaDateTime as DateTime</SourceLine>
    <SourceLine>Var LocalDateTimeString as String</SourceLine>
    <SourceLine>Dim FlightLogFile_fd as FolderItem</SourceLine>
    <SourceLine>dim tos as TextOutputStream</SourceLine>
    <SourceLine>Var NewFileName as string</SourceLine>
    <SourceLine>Var TimeNumberOfCharacters as int32</SourceLine>
    <SourceLine>Var LongitudeNumberOfCharacters as int32</SourceLine>
    <SourceLine>Var NewCells as String</SourceLine>
    <SourceLine>Var NumberOfFlightsDownloaded as Integer</SourceLine>
    <SourceLine>Var UpdateRateSlowDownCounter as Integer</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>NumberOfFlightsFound = 0</SourceLine>
    <SourceLine>SendCommand_Meth("l", SendCommandWithNoParameter)   '  Send L command (list flight logs) with no parameters</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>Thread1Error = ""</SourceLine>
    <SourceLine>If f &lt;&gt; Nil Then</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>ReceivedCharacters = ""</SourceLine>
    <SourceLine>ReceivedListOfFlights = ""</SourceLine>
    <SourceLine>StartTimeTicks = Ticks</SourceLine>
    <SourceLine>EndTimeTicks = StartTimeTicks + 1000     ' 16.7 second timeout. Mia's EEPROM can hold many flights with up to 8191 records.</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>while(ReceivedCharacters.Right(1) &lt;&gt; "&gt;" and (Ticks &lt; EndTimeTicks))   '  results of L command</SourceLine>
    <SourceLine>ReceivedCharacters = Serial1.ReadAll()</SourceLine>
    <SourceLine>ReceivedListOfFlights = ReceivedListOfFlights + ReceivedCharacters</SourceLine>
    <SourceLine>wend</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>NumberOfFlightsFound = ubound(ReceivedListOfFlights.split(":        "))</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>if NumberOfFlightsFound &gt;= 1 Then</SourceLine>
    <SourceLine>Me.AddUserInterfaceUpdate("DownloadCount":(0))</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>NumberOfFlightsDownloaded = 0</SourceLine>
    <SourceLine>Var MiaUSBNameDashIndex As Integer</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>// Now we know how many flight logs on the Mia, we need to download each one.</SourceLine>
    <SourceLine>// Since the Mia doesn't really have a file system on its logging EEPROM, the downloading of all but the first log requires the Mia's D command</SourceLine>
    <SourceLine>// to scan through all the earlier logs. So each download takes longer than the earlier one regardless of the logs length.</SourceLine>
    <SourceLine>for FlightNumber = 1 to NumberOfFlightsFound</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>ReceivedCharacters = Serial1.ReadAll()  ' flush the buffer</SourceLine>
    <SourceLine>SendCommand_Meth("d", FlightNumber)   '  Send D command with flight number parameters</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>ReceivedCharacters = ""</SourceLine>
    <SourceLine>ReceivedFlightLog = ""</SourceLine>
    <SourceLine>StartTimeTicks = Ticks</SourceLine>
    <SourceLine>EndTimeTicks = StartTimeTicks + 1200     ' 20 seconds timeout</SourceLine>
    <SourceLine>UpdateRateSlowDownCounter = 0</SourceLine>
    <SourceLine>while(ReceivedCharacters.Right(1) &lt;&gt; "&gt;" and (Ticks &lt; EndTimeTicks))   '  results of D command, grab all characters until we see our next prompt</SourceLine>
    <SourceLine>ReceivedCharacters = Serial1.ReadAll()</SourceLine>
    <SourceLine>ReceivedFlightLog = ReceivedFlightLog + ReceivedCharacters</SourceLine>
    <SourceLine>if (UpdateRateSlowDownCounter mod 40) = 0 then</SourceLine>
    <SourceLine>Me.AddUserInterfaceUpdate("DownloadCount":NumberOfFlightsDownloaded)</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine>UpdateRateSlowDownCounter = UpdateRateSlowDownCounter + 1</SourceLine>
    <SourceLine>thread.SleepCurrent (7)</SourceLine>
    <SourceLine>wend</SourceLine>
    <SourceLine>'msgbox(ReceivedFlightLog)</SourceLine>
    <SourceLine>'msgbox(str(len(ReceivedFlightLog)))</SourceLine>
    <SourceLine>' First, build the file name</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>LogFields = ReceivedFlightLog.ToArray(",")  // BE AWARE, this will combine the last field of a line to the first field of the next line with a carrage return as the seperation!</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>// Identify which Mia was the source of the data</SourceLine>
    <SourceLine>MiaUSBNameDashIndex = MiaUSBName.IndexOf("-")</SourceLine>
    <SourceLine>LogFields(1) = "Mia " + MiaUSBName.RightBytes(MiaUSBName.Length - MiaUSBNameDashIndex - 1)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>//msgbox(LogFields(6))  ' DEBUG  end of lines still OK here</SourceLine>
    <SourceLine>'msgbox(LogFields(19))  ' DEBUG  end of lines still OK here</SourceLine>
    <SourceLine>UNIXMiaTime = LogFields(6).ToInt64  '  extract Mia's last known time (.int64 will ignore the end of line, and header text)</SourceLine>
    <SourceLine>UNIXFlightTime = LogFields(21).ToInt64  '  extract flight time and convert to int64</SourceLine>
    <SourceLine>'msgbox(str(UNIXMiaTime - UNIXFlightTime))</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>'  Both the Mia "last known time" at LogFields(6) and the "flight time" at LogFields(19) are UNIX time, relitive to UTC.</SourceLine>
    <SourceLine>'  Excel is STILL clueless about time zones (things move very slowly at Microsoft!), so we must fix that here.</SourceLine>
    <SourceLine>FlightDateTime = UNIXTimeToLocalTime(UNIXFlightTime)</SourceLine>
    <SourceLine>MiaDateTime = UNIXTimeToLocalTime(UNIXMiaTime)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>' Now we have to add human readable date and time in the current time zone and daylight time state</SourceLine>
    <SourceLine>'     first, Mia's current time. We need to insert the human readable dateTime after the UNIX time</SourceLine>
    <SourceLine>TimeNumberOfCharacters = LogFields(6).IndexOf(chrb(13))</SourceLine>
    <SourceLine>LogFields(6) = LogFields(6).LeftBytes(TimeNumberOfCharacters) + "," + TimeSecondsToCellString(MiaDateTime) + LogFields(6).RightBytes(lenb(LogFields(6)) - TimeNumberOfCharacters)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>'   next, the flight time</SourceLine>
    <SourceLine>LongitudeNumberOfCharacters = LogFields(24).IndexOf(chrb(13))</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>// We insert some extra csv fields after the 'spare' field in the initial conditions</SourceLine>
    <SourceLine>NewCells = LogFields(24).LeftBytes(LongitudeNumberOfCharacters + 2) + ",,,,,," + TimeSecondsToCellString(FlightDateTime) + LogFields(24).RightBytes(lenb(LogFields(24)) - (LongitudeNumberOfCharacters + 1))</SourceLine>
    <SourceLine>LogFields(24) = NewCells</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>'LogFields(19) = FlightDateTime.SecondsFrom1970.ToString("###########.")</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>ReceivedFlightLog = String.FromArray(LogFields, ",")  '  &lt;-- EndOfLine chars are getting changed</SourceLine>
    <SourceLine>ReceivedFlightLog = ReceivedFlightLog.ReplaceLineEndings(EndOfLine.CRLF)</SourceLine>
    <SourceLine>'  https://forum.xojo.com/t/array-to-memoryblock/19707/2</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>// Make new file name with date-time string in local time</SourceLine>
    <SourceLine>LocalDateTimeString = TimeSecondsToFileString(FlightDateTime)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>NewFileName = "Mia_" + LocalDateTimeString + ".csv"</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>FlightLogFile_fd = f.child(NewFileName)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>//  open output file for write</SourceLine>
    <SourceLine>try</SourceLine>
    <SourceLine>tos = TextOutputStream.Create(FlightLogFile_fd)</SourceLine>
    <SourceLine>tos.write ReceivedFlightLog.RightBytes(lenb(ReceivedFlightLog) - 5)   '   remove the echoed characters from the D command</SourceLine>
    <SourceLine>'              (this leaves the last prompt at the bottom)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>tos.Flush   '  Make sure everything is written out</SourceLine>
    <SourceLine>tos.Close</SourceLine>
    <SourceLine>NumberOfFlightsDownloaded = NumberOfFlightsDownloaded + 1</SourceLine>
    <SourceLine>Me.AddUserInterfaceUpdate("DownloadCount":NumberOfFlightsDownloaded)</SourceLine>
    <SourceLine>Catch ErrorInfo as IOException</SourceLine>
    <SourceLine>Thread1Error = Thread1Error + NewFileName + ", "</SourceLine>
    <SourceLine>End Try</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>next</SourceLine>
    <SourceLine>if NumberOfFlightsDownloaded &lt;&gt; 0 then</SourceLine>
    <SourceLine>Me.AddUserInterfaceUpdate("DownloadCount":(-NumberOfFlightsDownloaded))</SourceLine>
    <SourceLine>else</SourceLine>
    <SourceLine>Me.AddUserInterfaceUpdate("DownloadCount":(-100001))</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>else</SourceLine>
    <SourceLine>// Failed to open download folder</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End If</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
  <HookInstance>
   <ItemName>UserInterfaceUpdate</ItemName>
   <Compatibility></Compatibility>
   <Attributes>API2Only</Attributes>
   <Visible>1</Visible>
   <PartID>2145409023</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub UserInterfaceUpdate(data() as Dictionary)</SourceLine>
    <SourceLine>var DownloadReturnValue as integer</SourceLine>
    <SourceLine>Var L As Integer</SourceLine>
    <SourceLine>Const ActivitySpinnerCharacters As String = " "    //   alt strings:  "-\|/"   "/|\\|/"    ""     ""   ""   ""</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>if DownloadActivityCounter = 3 then</SourceLine>
    <SourceLine>L=2</SourceLine>
    <SourceLine>else</SourceLine>
    <SourceLine>L=1</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>For Each update As Dictionary In data</SourceLine>
    <SourceLine>If update. HasKey("DownloadCount") Then</SourceLine>
    <SourceLine>DownloadReturnValue = update.Value ("DownloadCount").Integervalue</SourceLine>
    <SourceLine>if (DownloadReturnValue &lt; 0) and (DownloadReturnValue &gt; (-100000)) then</SourceLine>
    <SourceLine>NumberOfFlightsFoundLabel.Text = "Downloaded " + str(-(update.Value ("DownloadCount").Integervalue)) + " flights in total."</SourceLine>
    <SourceLine>CheckLocationBtn.Enabled = True</SourceLine>
    <SourceLine>UpdateSealevelPressureBtn.Enabled = True</SourceLine>
    <SourceLine>UpdateLocationBtn.Enabled = True</SourceLine>
    <SourceLine>DisconnectFromMiaButton.Enabled = True</SourceLine>
    <SourceLine>elseif DownloadReturnValue = 0 then</SourceLine>
    <SourceLine>NumberOfFlightsFoundLabel.Text = "Found flights. " + ActivitySpinnerCharacters.Middle(DownloadActivityCounter ,1 )</SourceLine>
    <SourceLine>elseif DownloadReturnValue = (-100001) Then</SourceLine>
    <SourceLine>NumberOfFlightsFoundLabel.Text = "No flights found."</SourceLine>
    <SourceLine>CheckLocationBtn.Enabled = True</SourceLine>
    <SourceLine>UpdateSealevelPressureBtn.Enabled = True</SourceLine>
    <SourceLine>UpdateLocationBtn.Enabled = True</SourceLine>
    <SourceLine>DisconnectFromMiaButton.Enabled = True</SourceLine>
    <SourceLine>else</SourceLine>
    <SourceLine>NumberOfFlightsFoundLabel.Text = "Flight " + str(update.Value ("DownloadCount").Integervalue) + " downloaded. " + ActivitySpinnerCharacters.Middle(DownloadActivityCounter , L)</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine>Next</SourceLine>
    <SourceLine>DownloadActivityCounter = (DownloadActivityCounter + 1) Mod 4</SourceLine>
    <SourceLine>NumberOfFlightsFoundLabel.Refresh</SourceLine>
    <SourceLine>MiaHost.Refresh</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
 </ControlBehavior>
