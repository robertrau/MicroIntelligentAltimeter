   <SourceLine>Sub UpdateWindowsLocation(l as WindowsLocationMBS)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>#if TargetWindows then</SourceLine>
   <SourceLine>dim Latitude as Double</SourceLine>
   <SourceLine>dim Longitude as double</SourceLine>
   <SourceLine>dim LocationAvailable as Boolean</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dim Altitude as Double</SourceLine>
   <SourceLine>dim AltitudeAvailable as Boolean</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>#Pragma BreakOnExceptions false</SourceLine>
   <SourceLine>try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>LatitudeWinAutomatic = l.Latitude</SourceLine>
   <SourceLine>LongitudeWinAutomatic = l.Longitude</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>AutomaticLocationAvailable = True</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>catch e as WindowsLocationExceptionMBS</SourceLine>
   <SourceLine>msgbox("ShowLocation:  Getting lat &amp; log threw WindowsLocationExceptionMBS")</SourceLine>
   <SourceLine>end try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>#Pragma BreakOnExceptions true</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>ShowLocation_Windows()</SourceLine>
   <SourceLine>return </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>#EndIf</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
