 <Method>
  <ItemName>CleanStringMeth</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1826873343</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function CleanStringMeth(MyText As String) As String</SourceLine>
   <SourceLine>dim InProcessText as String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>InProcessText = MyText.Trim(chrb(13), chrb(10), " ", chrb(9), "&gt;")</SourceLine>
   <SourceLine>Return InProcessText</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>MyText As String</ItemParams>
  <ItemResult>String</ItemResult>
 </Method>
 <Method>
  <ItemName>UpdateCheckboxesFromUserConfigurationLW</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>40933375</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub UpdateCheckboxesFromUserConfigurationLW()</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>//UserConfigurationLongWord</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if BitwiseAnd(UserConfigurationLongWord , VoltageOnP3NOTBitMask) = 0 then</SourceLine>
   <SourceLine>VoltageOnP3CB.Value = true</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>VoltageOnP3CB.Value = false</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if BitwiseAnd(UserConfigurationLongWord , SounderRequiresFreqNOTBitMask) = 0 then</SourceLine>
   <SourceLine>SounderRequiresFreqCB.Value = true</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>SounderRequiresFreqCB.Value = false</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if BitwiseAnd(UserConfigurationLongWord , ServoEnableBitMask) = 0 then</SourceLine>
   <SourceLine>ServoEnableCB.Value = true</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>ServoEnableCB.Value = false</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>//diag2.Text = UserConfigurationLongWord.ToHex(8)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
