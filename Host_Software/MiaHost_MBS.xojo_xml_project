   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Sub ManualLatitudeUpdate()</SourceLine>
   <SourceLine>Var CandidateLatitude as Double</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>CandidateLatitude = val(ManualLocationLatTF.Text)</SourceLine>
   <SourceLine>if (CandidateLatitude &gt;= -90) and (CandidateLatitude &lt;= 90) then</SourceLine>
   <SourceLine>CurrentLatitude = CandidateLatitude</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>ManualLocationLatTF.TextColor = rgb(200,0,0)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>End Sub</SourceLine>
