  <HookInstance>
   <ItemName>Pressed</ItemName>
   <Compatibility></Compatibility>
   <Visible>1</Visible>
   <PartID>65941503</PartID>
   <ItemSource>
    <TextEncoding>134217984</TextEncoding>
    <SourceLine>Sub Pressed()</SourceLine>
    <SourceLine>//  Get sealevel pressure from the closest NWS observation station.</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>var GridURL as string    //   example:  https://api.weather.gov/gridpoints/DTX/48,27/stations</SourceLine>
    <SourceLine>var StationURL as String   //  example: https://api.weather.gov/stations/kyip/observations/latest</SourceLine>
    <SourceLine>Var StationIDs() As String</SourceLine>
    <SourceLine>Var dummy as double</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>//lm.RequestPermissions(self)    //  needed</SourceLine>
    <SourceLine>// do we need to wait after request?</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>GridURL = NWS_LatLog_to_grid_url</SourceLine>
    <SourceLine>//GridURL = "ERROR"   //  for validation</SourceLine>
    <SourceLine>if GridURL &lt;&gt; "ERROR" then</SourceLine>
    <SourceLine>SealevelPressureInfo.TextColor = rgb(0,0,0)</SourceLine>
    <SourceLine>//   add error detect for NWS_LatLog_to_grid_url() returning a failure        fix</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>StationURL = NWS_grid_to_station_url(GridURL)</SourceLine>
    <SourceLine>StationIDs = StationURL.split("/")</SourceLine>
    <SourceLine>SealevelPressureInfo.Text = "Weather data from NWS station: " + StationIDs(4) + ".  "</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>SeaLevelPressure_Pa = NWS_station_to_measurements(StationURL)</SourceLine>
    <SourceLine></SourceLine>
    <SourceLine>if DisconnectFromMiaButton.Enabled = True then</SourceLine>
    <SourceLine>SendCommand_Meth("t", SeaLevelPressure_Pa)</SourceLine>
    <SourceLine>SealevelPressureInfo.Text = SealevelPressureInfo.Text + " Mia updated."</SourceLine>
    <SourceLine>else</SourceLine>
    <SourceLine>//SealevelPressureInfo.Text = SealevelPressureInfo.Text + " Sealevel pressure is " + str(SeaLevelPressure_Pa / 100) + " hPa."   //  hPa is the same units you adjust on the OLED</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine>//SealevelPressureInfo2.Text = "Updated sealevel pressure in Mia to " + str(SeaLevelPressure_Pa / 100) + " hPa"</SourceLine>
    <SourceLine>else</SourceLine>
    <SourceLine>SealevelPressureInfo.Text = "Weather data from NWS unavailable. Mia not updated."</SourceLine>
    <SourceLine>SealevelPressureInfo.TextColor = rgb(200,0,0)</SourceLine>
    <SourceLine>end if</SourceLine>
    <SourceLine>End Sub</SourceLine>
   </ItemSource>
  </HookInstance>
 </ControlBehavior>
