 <Method>
  <ItemName>NWS_url_to_json</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>920426495</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function NWS_url_to_json(URL as string) As string</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dim curlStatus as integer</SourceLine>
   <SourceLine>dim d as new CURLSMBS</SourceLine>
   <SourceLine>d.ClearData</SourceLine>
   <SourceLine>// Collect connection debug info</SourceLine>
   <SourceLine>//d.CollectDebugMessages = True</SourceLine>
   <SourceLine>//d.OptionVerbose = true</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>d.CollectOutputData = true</SourceLine>
   <SourceLine>d.SetOptionHTTPHeader array("User-Agent: test/1.0")   //  added 11/30/2024 per Christian @ MBS</SourceLine>
   <SourceLine>d.OptionIPResolve = d.kIPRESOLVE_V4  // prevent ipv6 as NWS has had some trouble with ipv6</SourceLine>
   <SourceLine>d.OptionFollowLocation = true   // required</SourceLine>
   <SourceLine>d.OptionMaxRedirs = 5             //  5 may be excessive</SourceLine>
   <SourceLine>d.OptionSSLVerifyHost = 2 // verify server</SourceLine>
   <SourceLine>d.OptionSSLVerifyPeer = 1 // proofs certificate is authentic</SourceLine>
   <SourceLine>d.OptionURL = URL</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Find our security cetificates</SourceLine>
   <SourceLine>#If TargetLinux Then</SourceLine>
   <SourceLine>dim cacert as FolderItem = findFile("cacert.pem")</SourceLine>
   <SourceLine>if cacert &lt;&gt; Nil then</SourceLine>
   <SourceLine>d.OptionCAInfo = cacert.UnixpathMBS</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>// Throw error</SourceLine>
   <SourceLine>msgbox("Unable to find SSL certificate file.")</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>#Else</SourceLine>
   <SourceLine>call d.UseSystemCertificates()</SourceLine>
   <SourceLine>#EndIf</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>//  Do the curl!!!</SourceLine>
   <SourceLine>curlStatus = d.Perform   // </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>//  collect the curl results and return</SourceLine>
   <SourceLine>var curlResultRaw as String</SourceLine>
   <SourceLine>var APIResult1 as String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>curlResultRaw = d.OutputData</SourceLine>
   <SourceLine>APIResult1 = ReplaceLineEndings(curlResultRaw, EndOfLine)  // </SourceLine>
   <SourceLine>return APIResult1</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>URL as string</ItemParams>
  <ItemResult>string</ItemResult>
 </Method>
 <Method>
  <ItemName>NWS_grid_to_station_url</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1561276415</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function NWS_grid_to_station_url(URL as string) As string</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// we only look for stations that make observations</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dim APIResult1 as String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// url support</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// make the NWS inquiry</SourceLine>
   <SourceLine>var curlResultRaw as String</SourceLine>
   <SourceLine>curlResultRaw = NWS_url_to_json(URL)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>APIResult1 = ReplaceLineEndings(curlResultRaw, EndOfLine)  // </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Must filter out NWS junk. The json sometimes has lines that start with:  "* TLSv1.2 (IN),..." and must be removed</SourceLine>
   <SourceLine>// example:</SourceLine>
   <SourceLine>'}</SourceLine>
   <SourceLine>'},</SourceLine>
   <SourceLine>'* TLSv1.2 (IN), TLS header, Supplemental data (23):</SourceLine>
   <SourceLine>'{</SourceLine>
   <SourceLine>'* TLSv1.2 (IN), TLS header, Supplemental data (23):</SourceLine>
   <SourceLine>'"id": "https://api.weather.gov/stations/KFNT",</SourceLine>
   <SourceLine>'"type": "Feature",</SourceLine>
   <SourceLine>'"geometry": {</SourceLine>
   <SourceLine>var LineArray() as String</SourceLine>
   <SourceLine>var ArrayIndex as Integer</SourceLine>
   <SourceLine>LineArray = APIResult1.Split(EndOfLine)</SourceLine>
   <SourceLine>ArrayIndex = 0</SourceLine>
   <SourceLine>For Each SingleLine As String In LineArray</SourceLine>
   <SourceLine>if leftb(SingleLine,1) = "*" then</SourceLine>
   <SourceLine>LineArray.RemoveAt(ArrayIndex)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>ArrayIndex = ArrayIndex + 1</SourceLine>
   <SourceLine>Next</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>//  now we have valid json text to make our json type</SourceLine>
   <SourceLine>Var JSON As New JSONItem (APIResult1)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Find station that has observations</SourceLine>
   <SourceLine>var StillLookingForStationWithObservations as Integer</SourceLine>
   <SourceLine>var StationURL as String</SourceLine>
   <SourceLine>var x as Integer</SourceLine>
   <SourceLine>var StationType as String</SourceLine>
   <SourceLine>StillLookingForStationWithObservations = 1</SourceLine>
   <SourceLine>x = 0</SourceLine>
   <SourceLine>do</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>StationType = JSON.Child("features").ChildAt(x).Child("properties").Value("@type") </SourceLine>
   <SourceLine>//msgbox(StationType)</SourceLine>
   <SourceLine>if StationType.IndexOfBytes("bservation") &gt; 0 then    //   looking for "wx:ObservationStation"</SourceLine>
   <SourceLine>StationURL = JSON.Child("features").ChildAt(x).Child("properties").Value("@id")</SourceLine>
   <SourceLine>StillLookingForStationWithObservations = 0</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>x = x + 1</SourceLine>
   <SourceLine>loop until  (StillLookingForStationWithObservations = 0)</SourceLine>
   <SourceLine>StationURL = StationURL + "/observations/latest"</SourceLine>
   <SourceLine>//msgbox(StationURL)</SourceLine>
   <SourceLine>return StationURL</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>URL as string</ItemParams>
  <ItemResult>string</ItemResult>
 </Method>
 <Method>
  <ItemName>NWS_station_to_measurements</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>1243326463</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function NWS_station_to_measurements(URL as string) As integer</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Given a NWS station URL we extract sealevel pressure from the json response.</SourceLine>
   <SourceLine>//   Some stations do not report sealevel pressure and we need to derive it from barametric pressure, elevation, and temperature.</SourceLine>
   <SourceLine>// See   https://www.weather.gov/documentation/services-web-api#/default/station_observation_latest</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dim APIResult1 as String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// url support</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// make the NWS inquiry</SourceLine>
   <SourceLine>var curlResultRaw as String</SourceLine>
   <SourceLine>curlResultRaw = NWS_url_to_json(URL)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>APIResult1 = ReplaceLineEndings(curlResultRaw, EndOfLine)  // </SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Must filter out NWS junk. The json sometimes has lines that start with:  "* TLSv1.2 (IN),..." and must be removed</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>var LineArray() as String</SourceLine>
   <SourceLine>var ArrayIndex as Integer</SourceLine>
   <SourceLine>LineArray = APIResult1.Split(EndOfLine)</SourceLine>
   <SourceLine>ArrayIndex = 0</SourceLine>
   <SourceLine>For Each SingleLine As String In LineArray</SourceLine>
   <SourceLine>if leftb(SingleLine,1) = "*" then</SourceLine>
   <SourceLine>LineArray.RemoveAt(ArrayIndex)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>ArrayIndex = ArrayIndex + 1</SourceLine>
   <SourceLine>Next //            UMMMMMM   none of this is getting used?????????????</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>//  now we have valid json text to make our json type</SourceLine>
   <SourceLine>Var JSON As New JSONItem (APIResult1)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Find station that has observations</SourceLine>
   <SourceLine>var x as Integer</SourceLine>
   <SourceLine>var StationType as String</SourceLine>
   <SourceLine>Var SeaLevelPressure As single</SourceLine>
   <SourceLine>var SeaLevelPressureQuality as String</SourceLine>
   <SourceLine>Var LocalPressure_Pa As Double</SourceLine>
   <SourceLine>Var LocalElevation_m As Double</SourceLine>
   <SourceLine>Var LocalElevation_feet As Integer</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>LocalPressure_Pa = JSON.Child("properties").Child("barometricPressure").Value("value")</SourceLine>
   <SourceLine>MiaHost.LocalGroundLevelPresLabel.Text = "Local ground level pressure: " + str(LocalPressure_Pa / 100) + "mb"</SourceLine>
   <SourceLine>LocalElevation_m = JSON.Child("properties").Child("elevation").Value("value")</SourceLine>
   <SourceLine>LocalElevation_feet = LocalElevation_m * 3.28084</SourceLine>
   <SourceLine>MiaHost.LocalGroundLevelAltLabel1.Text = "Local ground level altitude: " + str(LocalElevation_m) + "m  or " + str(LocalElevation_feet) + "ft."</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>//  See if sealevel pressure is provided</SourceLine>
   <SourceLine>SeaLevelPressureQuality = JSON.Child("properties").Child("seaLevelPressure").Value("qualityControl")</SourceLine>
   <SourceLine>if SeaLevelPressureQuality = "V" then    //  only use if Verified, passed levels 1, 2, and 3 (see comments below)</SourceLine>
   <SourceLine>SeaLevelPressure = JSON.Child("properties").Child("seaLevelPressure").Value("value")</SourceLine>
   <SourceLine>SealevelPressureInfo.TextColor = rgb(0,0,0)</SourceLine>
   <SourceLine>SealevelPressureInfo.Text = SealevelPressureInfo.Text + "Sealevel pressure available. "</SourceLine>
   <SourceLine>MiaHost.SealevelPressureLabel.Text = "Sealevel pressure: " + str(SeaLevelPressure / 100) + "mb"</SourceLine>
   <SourceLine>return floor(SeaLevelPressure + 0.5)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>Var LocalTemperature_C As Double</SourceLine>
   <SourceLine>Var SeaLevelPressureDerived As Double</SourceLine>
   <SourceLine>Var ReferenceTemperature As Double   //  at sea level, given our lapse rate of 0.0065K/m and our temperature at our ground level</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// seaLevelPressure is not available, we must derive it from other data the station provides.</SourceLine>
   <SourceLine>LocalTemperature_C = JSON.Child("properties").Child("temperature").Value("value")</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>//msgbox("Local elevation " +str(LocalElevation_m))</SourceLine>
   <SourceLine>//msgbox("Local pressure " +str(LocalPressure_Pa))</SourceLine>
   <SourceLine>//msgbox("Local temperature " +str(LocalTemperature_C))</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>//  THIS FORMULA BELOW DOESN'T AGREE WITH READINGS FROM NWS!!!!!</SourceLine>
   <SourceLine>//SeaLevelPressureDerived = LocalPressure_Pa / (Pow((1.0 - 2.25577e-5 * LocalElevation_m), 5.25588))   //  see: https://www.engineeringtoolbox.com/air-altitude-pressure-d_462.html </SourceLine>
   <SourceLine>ReferenceTemperature = LocalTemperature_C + (LocalElevation_m * 0.0065) + 273.15</SourceLine>
   <SourceLine>//ReferenceTemperature = LocalTemperature_C + 273.15</SourceLine>
   <SourceLine>//SeaLevelPressureDerived = LocalPressure_Pa * Pow(1.0 - ((0.0065 / ReferenceTemperature) * (-LocalElevation_m)), 5.25588)</SourceLine>
   <SourceLine>SeaLevelPressureDerived = LocalPressure_Pa * Pow(1.0 - ((0.0000225577) * (-LocalElevation_m)), 5.25588)</SourceLine>
   <SourceLine>SealevelPressureInfo.TextColor = rgb(200,0,0)</SourceLine>
   <SourceLine>MiaHost.SealevelPressureLabel.Text = "Sealevel pressure: " + str(SeaLevelPressureDerived / 100)</SourceLine>
   <SourceLine>SealevelPressureInfo.Text = SealevelPressureInfo.Text + "QC descriptor:" + SeaLevelPressureQuality + ", sealevel pressure Derived. "</SourceLine>
   <SourceLine>return floor(SeaLevelPressureDerived + 0.5)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// From the NWM github issues (answers to questions I asked):</SourceLine>
   <SourceLine>'You have the right order of operations. The observationStations property of the /points query gives you the /gridpoint/.../stations</SourceLine>
   <SourceLine>'link so you don't have to build your own gridpoints URL.</SourceLine>
   <SourceLine>'You can also cache the results of the earlier queries as they change very infrequently.</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'Be aware that not all stations report SLP or report it at every interval.</SourceLine>
   <SourceLine>'ASOS stations (usually large airports) are your best bet for consistent SLP observations.</SourceLine>
   <SourceLine>'Otherwise you may have to convert from station barometric pressure or altimeter setting, depending on what's available.</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'Reversing altimeter to station pressure is simple, but calculating sea level pressure is complicated.</SourceLine>
   <SourceLine>'At a minimum you will need the station altitude. Ideally you will also want temperature and humidity</SourceLine>
   <SourceLine>'(and/or dewpoint; with 2 of the values you can calculate the 3rd with psychometric formulas).</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'General meteorologic formulas can be found at:</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'https://www.weather.gov/epz/wxcalc</SourceLine>
   <SourceLine>'A formula for getting station pressure from altimeter is here:</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'https://www.weather.gov/media/epz/wxcalc/stationPressure.pdf</SourceLine>
   <SourceLine>'Converting station pressure to sea level pressure is more complex, depending on the level of accuracy you want.</SourceLine>
   <SourceLine>'There are corrections for temperature and humidity (and air density, gravity, and the aspheric nature of the Earth if you want to get crazy),</SourceLine>
   <SourceLine>'and some of the constants are from precomputed tables. The best sources I can find for this are:</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'https://en.wikipedia.org/wiki/Barometric_formula</SourceLine>
   <SourceLine>'https://integritext.net/DrKFS/correctiontosealevel.htm</SourceLine>
   <SourceLine>'Davis technote on derived values in their weather station products</SourceLine>
   <SourceLine>'You may just have to choose or develop your own adjustment so your data is internally consistent.</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// If the station supports seaLevelPressure and it has a value prepared for you, then you will see a JOSN leaf like this:</SourceLine>
   <SourceLine>//  "seaLevelPressure": {</SourceLine>
   <SourceLine>//      "unitCode": "wmoUnit:Pa",</SourceLine>
   <SourceLine>//      "value": 100980,</SourceLine>
   <SourceLine>//      "qualityControl": "V"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// If a station can't provide seaLevelPressure then you will see a JOSN leaf like this:</SourceLine>
   <SourceLine>//  "seaLevelPressure": {</SourceLine>
   <SourceLine>//      "unitCode": "wmoUnit:Pa",</SourceLine>
   <SourceLine>//      "value": null,</SourceLine>
   <SourceLine>//      "qualityControl": "Z"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// If the station can't provide seaLevelPressure then you can derive it from other data the station provides.</SourceLine>
   <SourceLine>// see https://integritext.net/DrKFS/correctiontosealevel.htm</SourceLine>
   <SourceLine>// and  https://en.wikipedia.org/wiki/Barometric_formula</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>//  qualityControl information from https://madis.ncep.noaa.gov/madis_sfc_qc_notes.shtml</SourceLine>
   <SourceLine>'------------------------------</SourceLine>
   <SourceLine>'MADIS QC Information - Surface</SourceLine>
   <SourceLine>'------------------------------</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'QC Data Descriptor Values</SourceLine>
   <SourceLine>'-------------------------</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'No QC available:</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'Z - Preliminary, no QC</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'Automated QC checks:</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'C - Coarse pass, passed level 1</SourceLine>
   <SourceLine>'S - Screened, passed levels 1 and 2</SourceLine>
   <SourceLine>'V - Verified, passed levels 1, 2, and 3</SourceLine>
   <SourceLine>'X - Rejected/erroneous, failed level 1</SourceLine>
   <SourceLine>'Q - Questioned, passed level 1, failed 2 or 3</SourceLine>
   <SourceLine>'</SourceLine>
   <SourceLine>'where level 1 = validity</SourceLine>
   <SourceLine>'level 2 = internal consistency, temporal consistency,</SourceLine>
   <SourceLine>'statistical spatial consistency checks</SourceLine>
   <SourceLine>'level 3 = spatial consistency check</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>URL as string</ItemParams>
  <ItemResult>integer</ItemResult>
 </Method>
