  <ItemName>NWS_LatLog_to_grid_url</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <PartID>630736895</PartID>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function NWS_LatLog_to_grid_url() As string</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>dim APIResult1 as String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// url support</SourceLine>
   <SourceLine>dim LatitudeFullText as String</SourceLine>
   <SourceLine>dim LatPointLocation as Integer</SourceLine>
   <SourceLine>dim LatitudeURLText as String</SourceLine>
   <SourceLine>dim LongitudeFullText as String</SourceLine>
   <SourceLine>dim LogPointLocation as Integer</SourceLine>
   <SourceLine>dim LongitudeURLText as String</SourceLine>
   <SourceLine>var NWS_URL as String</SourceLine>
   <SourceLine>Var NWSObservationStations As String</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// Build URL</SourceLine>
   <SourceLine>LatitudeFullText = str(CurrentLatitude)</SourceLine>
   <SourceLine>LatPointLocation = LatitudeFullText.IndexOf(".")</SourceLine>
   <SourceLine>LatitudeURLText = LatitudeFullText.left(LatPointLocation + 5)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>LongitudeFullText = str(CurrentLongitude)</SourceLine>
   <SourceLine>LogPointLocation = LongitudeFullText.IndexOf(".")</SourceLine>
   <SourceLine>LongitudeURLText = LongitudeFullText.left(LogPointLocation + 5)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>NWS_URL = "https://api.weather.gov/points/" + LatitudeURLText + "," + LongitudeURLText + "/"</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// make the NWS inquiry</SourceLine>
   <SourceLine>var curlResultRaw as String</SourceLine>
   <SourceLine>curlResultRaw = NWS_url_to_json(NWS_URL)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>// get our URL from the json</SourceLine>
   <SourceLine>APIResult1 = ReplaceLineEndings(curlResultRaw, EndOfLine)  // </SourceLine>
   <SourceLine>Var JSON As New JSONItem (APIResult1)</SourceLine>
   <SourceLine>//  I received this JSON and crashed:     must fix</SourceLine>
   <SourceLine>// {"correlationId":"fd152f04","title":"Data Unavailable For Requested Point","type":"https://api.weather.gov/problems/InvalidPoint","status":404,"detail":"Unable to provide data for requested point 44.6997,0","instance":"https://api.weather.gov/requests/fd152f04"}</SourceLine>
   <SourceLine>try</SourceLine>
   <SourceLine>NWSObservationStations = JSON.Child("properties").Value("observationStations")</SourceLine>
   <SourceLine>Catch </SourceLine>
   <SourceLine>NWSObservationStations = "ERROR"</SourceLine>
   <SourceLine>end try</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>return NWSObservationStations</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Function</SourceLine>
