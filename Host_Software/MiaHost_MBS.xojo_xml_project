   <SourceLine>ManualLocationLatLab.Visible = True</SourceLine>
   <SourceLine>ManualLocationLogLab.Visible = True</SourceLine>
   <SourceLine>ManualLocationLatTF.Visible = True</SourceLine>
   <SourceLine>ManualLocationLogTF.Visible = True</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>End Sub</SourceLine>
  </ItemSource>
