  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function HarvestByteValues() As String</SourceLine>
   <SourceLine>//  Pick up all the byte parameters, limit them, scale them, and return them as a long string. Pad unused bytes with FF</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>Var HexWordString As String</SourceLine>
   <SourceLine>Var WordAsInt As Integer</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>WordAsInt = Max(Min(val(ServoPreLaunchTF.text), 180), 0)</SourceLine>
   <SourceLine>HexWordString = WordAsInt.ToHex(2)</SourceLine>
   <SourceLine>WordAsInt = Max(Min(val(ServoAscentTF.text), 180), 0)</SourceLine>
   <SourceLine>HexWordString = HexWordString + WordAsInt.ToHex(2)</SourceLine>
   <SourceLine>WordAsInt = Max(Min(val(ServoApogeeTF.text), 180), 0)</SourceLine>
   <SourceLine>HexWordString = HexWordString + WordAsInt.ToHex(2)</SourceLine>
   <SourceLine>WordAsInt = Max(Min(val(ServoDecentTF.text), 180), 0)</SourceLine>
   <SourceLine>HexWordString = HexWordString + WordAsInt.ToHex(2)</SourceLine>
   <SourceLine>WordAsInt = Max(Min(val(ServoLandedTF.text), 180), 0)</SourceLine>
   <SourceLine>HexWordString = HexWordString + WordAsInt.ToHex(2)</SourceLine>
   <SourceLine>WordAsInt = Max(Min(val(ServoLowPowerTF.text), 180), 0)</SourceLine>
   <SourceLine>HexWordString = HexWordString + WordAsInt.ToHex(2)</SourceLine>
   <SourceLine>WordAsInt = val(SounderSlowDownTF.text)</SourceLine>
   <SourceLine>HexWordString = HexWordString + WordAsInt.ToHex(2)</SourceLine>
   <SourceLine>WordAsInt = floor(val(DelayBeforeLowPowerTF.text)/10.0 + 0.5)</SourceLine>
   <SourceLine>HexWordString = HexWordString + WordAsInt.ToHex(2)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>WordAsInt = Max(Min(val(ServoHCOutTF.text), 180), 0)</SourceLine>
   <SourceLine>HexWordString = HexWordString + WordAsInt.ToHex(2)</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>HexWordString = HexWordString + "FFFFFFFFFFFFFF"</SourceLine>
   <SourceLine>return HexWordString</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
